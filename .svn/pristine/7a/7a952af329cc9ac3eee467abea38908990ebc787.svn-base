    map.setZoom(map.getZoom() - 9);
    $('#stateResultDiv').hide();
    $('#zipnotfound').hide();
    $('#stateNotFound').hide();
    $("#cityCheck").prop("readonly", false);
    });
    $('#stateCheck').focusout(function(){
    var state_name = $('#stateCheck').val();
