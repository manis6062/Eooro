/* Appbuilder */

.icon-device-apple, .icon-device-android {
	background-image: url("images/appbuilder-icons.png");;
	background-repeat: no-repeat;
	height: 32px;
	width:27px;
	display:inline-block;
	cursor: pointer;
	margin:5px 10px;
} 

.icon-device-apple {
	background-position: -372px  -343px;
	&:hover, &.active {
		background-position: -272px -343px;
	}
}
.icon-device-android {
	background-position: -423px -343px;
	&:hover, &.active {
		background-position: -323px -343px;
	}
}


