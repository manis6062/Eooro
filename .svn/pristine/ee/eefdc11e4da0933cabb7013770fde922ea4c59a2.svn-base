.navicons {
	border-bottom: 1px solid #e5e5e5;
	ol {
		list-style: none;
		display: block;
		clear: both;
		width: 88%;
		margin: 30px auto 20px;
		position: relative;
		li {
			display:inline-block;
			width:123px;
			vertical-align: middle;
			text-align: center;
			font-size: 14px;
			font-weight: normal;	
			position: relative;		
			a {
				color: #ababab;
			}
			i {
				display: block;
				clear: both;
				width: 80px;
				height: 80px;
				margin: 0 auto 0 26px;
			}
			&.active, &:hover, &.checked:hover, &.checked.active, &.checked.active:hover {
				a {
					color:#2980b9;				
				}
			}
			&.checked {
				a {
					color:#36ce77;				
				}
			}
			&.active::before {
				content: "";
				height: 20px;
				width: 30px;
				background: url("images/appbuilder-icons.png") no-repeat -604px -340px transparent;
				position: absolute;
				bottom: -25px;
				left: 51px;

			}
		}
	}
}

.appbuilder {
	text-align: left;

	a.btn, .btn {
		background-color: transparent;
		border: 1px solid #727272;
		color: #727272;
		font-weight: 500;
		font-family: "Lato", sans-serif;
		&:hover {
			color: white;
			background-color:#727272;
		}
		&.btn-success {
			color: #27ae60;
			border-color: #27ae60;
			background-color: transparent;
			&:hover {
				color: white;
				background-color:#27ae60;
			}
		}
		&.btn-info {
			color: #2980b9;
			border-color: #2980b9;
			background-color: transparent;
			&:hover {
				color: white;
				background-color:#2980b9;
			}
		}
		&.btn-default {
			color: #1ABC9C;
			border-color: #1ABC9C;
			background-color: transparent;
			&:hover {
				color: white;
				background-color:#1ABC9C;
			}
		}
	}
	a.btn-mini {
		padding:3px;
		text-transform: uppercase;
		color: #aaa;
		display: inline-block;
		margin-left: 10px;
		margin-top: 12px;
		font-size:12px;
		line-height: 1;
		cursor: pointer;
		float: right;
		&.btn-success {
			background-color: @success;
			color: white;
			padding:3px 10px 4px;
			&:hover {
				color: white;
				background-color: @success-hover;
			}
		}
		&:hover {
			color:@alert;
		}
	}
	article {
		width: 94%;
		margin: 0 auto;
		display: block;
		clear: both;
	}

	h4 {
		text-align:left;
		font-size: 16px;
		line-height: 1;
		margin:30px 0 0;
		color: #333333;
		font-weight: 400;
	}
	p {
		color:#999999;
		margin-bottom: 12px;
		line-height: 1.2em;	
		&.subheading {
			color: #333;
			font-size:14px; 
			margin-bottom: 16px;
		}
		&.alert-tip {
			color:@danger;
			margin:20px 0;
		}
		&.alert {
			color:@danger;
			margin:5px 0;
		}
		&.tip {
			padding 0;
			margin-bottom: 5px;
			margin-top: 10px;
		}
	}
	
	p.successMessage, p.errorMessage {
		margin: 20px auto 0 auto !important;
		display: table;
		clear: both;
		width: auto !important;
		text-align: center;
	}
	p.errorMessage {
		color:@danger;
	}
	p.successMessage {
		color:@success;
	}
	.loading-image {width:30px;padding:5px 0;text-align: center;}	
	.span600px .loading-image { width:100%;}	
	.sortable-add {
		background-position: 0 6px; 
		color:@success;
		&:hover {
			color:@success-hover;
		}
	}	
	.nicefile-all{ 
		.nicefile-input {
			max-width:270px;
		}
		.nicefile-button {
		    display: inline-block;
		    padding: 7px 10px;
