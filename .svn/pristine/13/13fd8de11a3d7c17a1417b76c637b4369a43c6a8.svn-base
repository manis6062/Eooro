  /* #Standard Button */
	/*.input-button-form, */
	a.input-button-form, 
	.bt-highlight  button, 
	.standardButton	button, 
	.button h2 a, 
	.content-faq button, 
	.standardButton > a   {
		padding:10px 30px; 
		color:#FFF; 
		background-color:#FF004F;
		text-transform: uppercase;  
		font-size:11px;
		/*line-height:16px;*/
		border:none;
    border-radius: 5px;
	}
p.standardButton > a.customStandardButton {
    margin: 5px 0;
    cursor: pointer;
}

	p.standardButton button.inactive, p.standardButton button.inactive:hover {background-color:#ccc; cursor:default;}
