/** DESIGN FIXES **/  
body {background-color:#061826; margin:0; padding:0;}
.main {margin: -20px 0;background-color: #ffffff; padding: 20px 20px 0}
.text-small {font-size:13px;}
.navbar-fixed-top {margin:0 0 20px 0;}
hr {margin: 2px 0 5px; }
h5 {color:#6d6d6d;}
h4 {line-height:17px; font-size:16px;}
.main .container {margin: 0 -10px 20px;}
.navbar-search .search-query {margin-top:2px;line-height: 15px;}
a.btn.btn-navbar, 
a.btn.btn-navbar:active, 
a.btn.btn-navbar:visited {height: 20px; width: 20px;   padding: 9px 7px 5px;margin: 5px 10px 5px 0; 
 background: -moz-linear-gradient( top, #46699c 0%,  #0f1d52);
    background: -webkit-gradient( linear, left top, left bottom, from(#0b314d), to(#061826));               
    border-radius: 4px; border: 0px solid rgb(50, 63, 73); box-shadow: inset 0px 0px 1px rgba(255,255,255,0.3);   
}
 
a.btn.btn-navbar:hover{
         background: -webkit-gradient(
            linear, left top, left bottom, 
            from(#0b314d),
            to(#061826));  
        box-shadow:                        
        inset 0px 0px 1px rgba(255,255,255,0.2);}

.footer .box-copyright p {margin-bottom: 0;}
.footer .box-copyright {margin-bottom: 5px;}

.pager li > a {background-color:#f5f5f5; color:#0170a8; border-color:#ddd; border-radius:3px; width:105px;max-width:105px;}
.pager li:first-child { margin-right:20px;}

.row-fluid {clear: both;}

div.img-polaroid.no-image {max-width: 168px;min-height: 60px;background-color:#f6f6f7;background-image: url('../img/noimage.png');background-repeat: no-repeat;background-position: center;}

/*Extras*/
.plusmarginb {margin-bottom:20px;}
.warning {text-indent: 0;padding: 5px;border-radius: 4px;color: rgb(209, 139, 62);border: 1px solid;    background-color: rgb(255, 253, 245);margin-bottom:40px;}
/*.warning:before{font-family: FontAwesome; content:"\f05a";height:14px; width:14px; float:left;    margin:2px;}*/

/*Error*/
form .errorMessage{text-indent: 0;padding: 5px;border-radius: 4px;color: rgb(209, 139, 62);border: 1px solid; background-color: rgb(255, 253, 245);}
/*.detail form .errorMessage:before{font-family: FontAwesome; content:"\f071";height:14px; width:14px; float:right;    }*/
    
/*Success*/
 form .successMessage{text-indent: 0;padding: 5px;border-radius: 4px;color:rgb(62, 162, 209);border: 1px solid;  background-color: rgb(245, 253, 255);}
/*.detail form .successMessage:before{    font-family: FontAwesome; content:"\f00c";height:14px; width:14px; float:right;    }*/

/*Infromation*/
.informationMessage{text-indent: 0;padding: 5px;border-radius: 4px;color:rgb(81, 163, 77);border: 1px solid;  background-color: rgb(245, 255, 249);}

/*Nav bar*/
.nav-tabs.nav-stacked { background-color: rgb(245, 245, 245); margin: -20px -10px 0 ;}
.nav-collapse .nav-tabs.nav-stacked {margin: 0 -20px 0 ;}
.nav-tabs.nav-stacked  a{color: rgb(124, 124, 124);}  
.nav-tabs.nav-stacked a > i{color: rgb(90, 90, 90);}  
.nav-collapse .nav-stacked li a >i {color: rgb(235, 235, 235);}  

.nav-tabs.nav-stacked > li > a {border-left:none;border-right:none;}
.nav-tabs.nav-stacked > li > a >i {margin-left:10px;}
.nav-collapse .nav-tabs.nav-stacked > li > a >i {margin-left:30px;}
.nav-stacked > li.divider {border-bottom: 1px solid #ddd}
.nav-collapse .nav-stacked > li.divider {border-bottom: none}
.navbar-inverse .nav-tabs.nav-stacked {background-color: transparent;}
.navbar .btn-navbar span {color:#fff;}
.navbar .btn-navbar .icon-bar {background-color:#fff;margin-top: 3px; margin-left: 1px;}

/*Home*/
.footer .control {padding-bottom: 15px; text-align: center;margin-top:-15px;}
.footer .control a.btn-info:first-child {margin-right:5%;}
.home-recents>h4{line-height:30px; margin-left:10px; font-size:16px;}
.home-recents hr{margin:-5px 10px 10px;}
.home-recents hr:last-child{display:none;}
.home-recents .recent-listing {padding-bottom:10px}
.home-recents .recent-listing > .image {margin: auto 13px 8px 10px;min-height: 53px;background-size: 100%;}

.home-recents .recent-listing .title {margin-left:10px;}
.home-recents .recent-listing .title>h4 {padding:0; margin:0; line-height:16px; }
.home-recents .recent-listing .title>address {line-height:14px;margin-top:5px; font-size:14px;}

/*Buttons*/                                                                
.btn-info {width:36%;background-color: #0170a8;background-image: none; border: none; box-shadow: none; white-space:nowrap; font-size:12px;}
.btn-info:hover,.btn-info:active {background-color: #0e4764}
.btn-primary {background-color: #f5f5f5; color:#0170a8; background-image: none; border: 1px solid #ddd; box-shadow: none;text-shadow:none;}
.btn-primary:hover,.btn-primary:active {background-color: #ddd;color:#0170a8; text-shadow:none;} 
.btn-primary.span12 {margin:-10px 0  15px 0;}
.btn-success {padding: 5px 40px; box-shadow:none; background-image:none; border:none;}


/*Thumbnail*/
.thumbnail {border-radius: 4px;    margin: -5px 0 25px; padding:0px 8px;clear: both;min-height: 60px;}
.thumbnail p{white-space:normal;}
.thumbnail address p{white-space:normal;}
.thumbnail .row-fluid h4{color:#000); text-transform:capitalize;}        
.thumbnail .row-fluid h5{line-height:17px;}        
.thumbnail .row-fluid h4  em {font-style:normal;font-size:15px;font-weight:normal;color:#333;line-height:20px;}
.thumbnail .price {margin-bottom: -30px;}
.thumbnail .row-fluid h4 span {    margin: -8px 0 0 0;    line-height: 0;    font-size:15px;    font-weight:normal;}

.itemView .image.span4.pull-left.img-polaroid {margin-right:10px; margin-bottom:5px;background-size: 100%;max-width: 158px;}

.listingView > .row-fluid > .image {margin-top:10px;margin-left:10px;}

/*Gallery */
.gallery {float:left;width:100%;margin:5px 0 12px 0;}
.gallery > a.span4 {margin-right:5%; width: 29%;}
.gallery > a:nth-child(3n){margin-right:0;margin-bottom:10px;}   
.gallery > a:last-child{clear:right;}   


/*Specials */

.blogView .blog-info .info,.blog-info .info {font-size:13px;line-height: 16px;margin-bottom:10px;}
.detail .blog-info {}
.blog-info .img-polaroid {margin-right:10px;}
.blog-info h4 {line-height: 13px;}

div.listingdetail {    margin:0;padding:0;}
      
.articles h4 span{width:80px;}
.articles h4 strong {float:left;}
.thumbnail .articles h4 .articleInfo {font-size:13px; color:#6d6d6d; font-weight:bold;line-height:15px;}
.eventView address {margin-bottom:10px;}    
.listings-summary {margin-bottom:10px;}

.listings-summary  .thumbnail, 
.articles-summary .thumbnail  {  position: relative;margin: 0;padding:0;background-color: white;border: 1px solid #DDD;    border-radius: 4px;    box-shadow:none;}
.navafter a{    font-weight:bold;text-transform: uppercase;font-size:12px}
.listings-summary .navafter, 
.articles-summary .navafter { background-color: #F7F7F9;  line-height: 32px;margin: -1px 0 0;padding: 0.2% 3%;text-align: right; border: 1px solid #E6E6E6;border-bottom-left-radius: 4px; border-bottom-right-radius: 4px;  }    
.articles-summary .navafter, .listings-summary  .navafter {margin: -1px -1px -1px}

.listings-summary  .thumbnail {margin-bottom:15px;}
.articles-summary .thumbnail {margin: -5px 0 25px 0;}
.articles-summary .thumbnail .row-fluid h4 {margin:3%;}   
.listings-summary  hr{margin:10px 0; padding:0;} 
.listings-summary address {margin:0 0;padding: 2px 1px 1px 3px;}
.listings-summary address p{font-size:13px; line-height:14px;}
.listings-summary  .thumbnail p,.articles-summary  .thumbnail p{margin:0 3% 3% ;}        
.listings-summary  .thumbnail h4{font-size: 16px; margin-left:10px;padding-right: 5px;}


.container h1{ color: #EEEEEE; float: left; font-size: 26px; margin: 0; padding: 0 0 0 10px; }
.container h1 a,
.container h1 a:visited{ color: #eee; }    

.container h1 a:hover{ text-decoration: none; }
.price > h4 ,.price>h4 span.cents{line-height: 12px;margin: 10px 0 35px;clear: right; font-weight:bold;}
.classified-info   .price h5 {font-weight:bold; font-size:16px; color:#000;height: 40px;}
    
/*Detail */
.detail{}
.detail .thumbnail{ margin:0 0 40px 0; border:none; padding:0;}
.detail .thumbnail address {margin-top:10px;}
.detail .thumbnail  {font-size:14px; font-weight:normal;}
.detail .images li{ float: left; margin: 10px 15px 0 0; width: 28%; }
.detail h4{ margin: 0 0 10px; }
.detail h5{ line-height: 2; margin: 20px 0 0; }

.detail .contact, .redeem .contact{ background: #EEEEEE; margin:5px -10px -20px; padding: 15px 25px 1px 20px; }
.detail .contact .control, .redeem .control {width:100%; text-align:center;}
.redeem  {margin:0 0 20px 0;}
.detail .contact label{ font-size: 13px; }

.detail .articleInfo{color:#6d6d6d;line-height:12px; font-weight:bold;}  
.detail p {clear:both;margin-top:10px; white-space: normal;}
.detail address p {text-indent:0;white-space: normal;}
.detail p.listing-description, .listingView p {text-indent:0;}
.listingView p {white-space:normal;}

.detail .deal-info{clear:both;float: left; width:100%;margin-bottom:20px;}
.detail .deal-info .img-polaroid {width: 31%;margin-right: 3px;background-size: 100%;}

.detail .deal-info ul { min-width:200px; max-height: 20px; background-color:#f5f5f5; margin:0; float:left; width:100%; padding:2px 0; font-size:10px;}
.detail .deal-info ul > li {margin:0; min-width:55px; float:left;margin-left:5px; list-style: none; vertical-align: middle; text-align: center;}
.detail .deal-info {font-size:11px; line-height:14px;}
.detail .deal-info .moreinfo {margin: 2px 10px 0 2px;}
.detail .deal-info .moreinfo .pull-left {margin-right:10px;}


h5.eventDateTime {color:#a2a2a2}
h5.eventDateTime span{font-weight:normal;}
/*Results*/
.results{}
.searchResults{font-size:12px; color:#5C6B77;line-height:20px;margin:0;}
.searchResults span {font-weight:bold;}
.generalResults { font-size: 13px;font-weight: bold;padding: 6px; clear:both; border-radius:4px; border: 1px solid #ddd; background-color:#f5f5f5;}
.generalResults a{text-transform: uppercase;}
.generalResults a:hover{text-decoration: none;}
.generalResults a.pull-right{text-transform: capitalize; font-weight:normal;}
.generalResults a.pull-right:after {  content:" >>"; color:#0088cc; }
.moreResults { text-align:right; border-bottom:1px solid rgb(243, 243, 243); padding-bottom:5px;margin-top: -20px;} 
p.moreResults a:after {  content:" >>"; color:#0088cc; }  

.results .image{ float: left; margin: -5px 20px 10px 0;  }    
.results .filter{ border-bottom: 1px solid #EEEEEE; display: block; margin: 0 0 25px; padding: 0 0 10px; }
.results .filter select{ width: 100%; }
.results .contact-info{ margin: 0 0 10px; padding: 0 0 0 110px; }
.results .contact-info span{ display: block; }
.results form {margin-top: 10px;}
.results .pager {margin:0 auto 15px}



.phone span, .phone {text-transform:capitalize;margin-top:-10px;}
.classifiedView .classified-info > h4 >a {clear:both;}
.detail h4.price, .detail h4.price span.cents {color:#004870;margin-bottom:20px; font-size:17px; font-weight:bold;}   

/*Forms*/
select {border:1px solid #ddd;  background:url("../img/arrow_select.png")  no-repeat 98% 13px #f5f5f5;
       padding: 0 0 0 10px; height: 33px; color:#000;-webkit-appearance: none; margin-bottom:20px;}
     
/*Condição para mudar a cor do icone no menu interno ( collapse) quando não estiver visível a Fontawesome*/       


.nav-collapse .nav-stacked li a .icon-home{background-position: 0 -24px; background-image:url("../img/glyphicons-halflings-white.png");}
.nav-collapse .nav-stacked li a .icon-briefcase{background-position: -432px -144px; background-image:url("../img/glyphicons-halflings-white.png");}
.nav-collapse .nav-stacked li a .icon-calendar{background-position: -192px -120px; background-image:url("../img/glyphicons-halflings-white.png");}
.nav-collapse .nav-stacked li a .icon-th-large{background-position: -216px 0; background-image:url("../img/glyphicons-halflings-white.png");}
.nav-collapse .nav-stacked li a .icon-file{background-position: -24px -24px; background-image:url("../img/glyphicons-halflings-white.png");}
.nav-collapse .nav-stacked li a .icon-tag{background-position:  0 -48px; background-image:url("../img/glyphicons-halflings-white.png");}
.nav-collapse .nav-stacked li a .icon-comment{background-position: -240px -120px; background-image:url("../img/glyphicons-halflings-white.png");}
 
}
@media only screenand (min-device-width : 480px) and (-webkit-min-device-pixel-ratio : 2){

.nav-collapse .nav-stacked li a .icon-home{background:none;}
.nav-collapse .nav-stacked li a .icon-briefcase{background:none;}
.nav-collapse .nav-stacked li a .icon-calendar{background:none;}
.nav-collapse .nav-stacked li a .icon-th-large{background:none;}
.nav-collapse .nav-stacked li a .icon-file{background:none;}
.nav-collapse .nav-stacked li a .icon-tag{background:none;}
.nav-collapse .nav-stacked li a .icon-comment{background:none;}
 
}