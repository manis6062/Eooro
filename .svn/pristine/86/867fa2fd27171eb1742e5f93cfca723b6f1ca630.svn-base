/* Rate Starts */
.stars-rating {
	background:url(images/iconography/stars-full.png) repeat-x  left center scroll transparent;
	display:inline-block;
	height: 14px;
	width:78px;
	margin-top: 3px;
	margin-bottom: -2px;
	[class*="rate-"] {
		height: 13px;
		float:right; 
		background:url(images/iconography/stars-empty.png) repeat-x  left center scroll @well-color;
	}
}


.large-rating {
	.stars-rating {
		background:url(images/iconography/large-stars-full.png) repeat-x  left center scroll transparent;
		display:inline-block;
		height: 25px;
		width:110px;
		margin-top: -6.8px;
		margin-bottom: -2px;
		.box-sizing(border-box);
		[class*="rate-"] {
			height: 25px;
			float:right; 
			background:url(images/iconography/large-stars-empty.png) repeat-x  left center scroll white;
			}
	}
}


/*BUTTONS */
.btn.btn-primary{
  background-color: @brand-primary;
