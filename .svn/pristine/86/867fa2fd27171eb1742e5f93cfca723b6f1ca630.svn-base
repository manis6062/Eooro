/**
* eDirectory - Members Section
*/

@import "variables.less";
@import "mixins.less";

h2 {
	font-size:20px;
	font-weight: normal;
}

.textcounter {
 border: 1px solid #ccc;
 padding:1px 5px !important;
 color: #000 !important;
}

/*Alert popup*/
#fancybox-outer {border-radius:0!important}

a:focus, a:active {border:none; outline: none;}

.review-name a {font-size:14px; font-weight:normal;color:#333;}
.review-name a:hover {color:#1abc9c;}

strong {font-weight:600;}

.member-form label > em {  color: #999999;  font-size: 10px;  font-style: normal;  font-weight: 400;}

.wrapcaption {font-size:12px;}

/*Texts*/
.muted, .text-muted {
	color:@gray-text;
	&:hover, &:focus {
		color:@gray-text;
	}
}
.text-success {
	color:@brand-success;
	&:hover, &:focus {
		color:@brand-success-hover;
	}
}
.text-info {
	color:@brand-info;
	&:hover, &:focus {
		color:@brand-info-hover;
	}
}
.text-warning {
	color:@brand-warning;
	&:hover, &:focus {
		color:@brand-warning-hover;
	}
}
.text-danger {
	color:@brand-danger;
	&:hover, &:focus {
		color:@brand-danger-hover;
	}
}

/*Alerts */
.alert {
  padding: 8px 35px 8px 14px;
  background-color: mix(@brand-waiting-hover, white, 15%);
  border: 1px solid mix(@brand-waiting-hover, white, 90%);
  text-shadow:none;
  color:@brand-waiting;
}

.alert-success {
  background-color: mix(@brand-success-hover, white, 15%);
  border: 1px solid  mix(@brand-success-hover, white, 90%);;
}
.alert-success, .alert-success .alert-heading {
  color: @brand-success;
}
.alert-danger, .alert-error {
  background-color: mix(@brand-danger-hover, white, 15%);
  border: 1px solid mix(@brand-danger-hover, white, 90%);
}
.alert-danger,
.alert-error,
.alert-danger .alert-heading,
.alert-error .alert-heading {
  color: @brand-danger;
}
.alert-info {
  background-color: mix(@brand-info-hover, white, 15%);
  border: 1px solid mix(@brand-info-hover, white, 90%);
}
.alert-info, .alert-info .alert-heading {
  color: @brand-info;
}

/*Menu Notification*/
.notify {
	position:absolute;
	width:20px;
	height:20px;
	background-color: @brand-primary;
	color: white;
	text-align: center;
	font-size: 10px;
	font-style: normal;
	font-weight: 600;
	right: 0;
	top: 0;
	border-radius: 20px;
	pointer-events:none;
}

.ie .notify, .ie .alert-new {
	background:url(images/iconography/ie-notification.png) no-repeat 0 0 transparent;
	font-size:9px;
	width:20px !important;
	height:20px;
	padding:0;

}

/*Login Options*/
.complementaryInfo {font-size:9px;}
.loginComplementaryInfo {text-align:center;}


.list-back {
	margin:20px 0;
}



/* #Buttons */

p.standardButton {text-align: center;margin: 15px 20px;display:inline;}
.baseButtons p.standardButton {margin: 15px 0;}


#img_customtime {padding:10px;display: inline;}


/* Admin Actions*/
.admin-actions {display:block;	width:99.6%; padding:10px 0 0; text-align: right;}

.admin-actions .admin-btn {
	background-color: @brand-primary;
	color: #FFFFFF;
	font-size: 11px;
	line-height: 16px;
	padding: 10px 30px;
	text-transform: uppercase;
	border:none;
}
.admin-actions .admin-btn:hover, .admin-actions .admin-btn:focus { background-color: @brand-primary-hover;}

