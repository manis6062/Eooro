/* DASHBOARD MEMBERS 2014*/


.webitem {
	border:1px solid @gray-light;
	.opacity(0.8);
	.transition(0.4s);
	.title {
		font-size: 1.1em;
	}
	p {
		font-size:0.9em;
		font-weight: 300;
	}
	b {
		font-weight: 600;
	}	
	.desc {
		background-color:white;
		p span {
			color:@brand-warning;
			font-size:0.95em;
			font-style: italic;
			font-weight: 600;
		}
		span.status {	
			position: relative;
			text-align: center;
			 > span {
			 	font-style: normal;
				font-weight: 300;
				font-size: 12px;
				cursor: default;
				&:before {
					content: "";
					display: block;
					margin: 0 auto;
					height:20px;
					width:20px;
					border-radius: 20px;
					overflow: hidden;					
				}
				&.status-active		{ 
					color:@brand-success; 
					&:before {
						background-color: @brand-success;
					}
				}
				&.status-suspended	{ 
					color:@brand-warning; 
					&:before {
							background-color: @brand-warning;
						}
				}	
				&.status-expired	{ 
					color:@brand-danger;
					&:before {
							background-color: @brand-danger;
						}
				}
				&.status-pending 	{ 
					color:@brand-waiting;
					&:before {
							background-color: @brand-waiting;
						}
				}
			 }
		}
	}
	.action {
		background-color:@gray-extralight;
		font-size:0.9em;
		font-weight: 300;
		color:@link-color;
	}
	&.active, &.active:hover{
		border-color: mix(@brand-highlight, white, 70%);
		box-shadow: 1px 2px 2px @gray-lighter;
		.opacity(1);
		.desc {
			background: white;
		}
	}
	&:hover {
		border-color: mix(black, white, 15%);
		box-shadow: 0 0 2px @gray;
		.desc {		
			cursor: pointer;
		}
	}
	
}	



.addcontent {
	border: 1px solid @gray-light;
	> p {
