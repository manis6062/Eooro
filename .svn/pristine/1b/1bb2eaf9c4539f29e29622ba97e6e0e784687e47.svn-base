
[class*="colorSelector"] {
	position: relative;
	width: 36px;
	height: 36px;
	background: url(../images/select_empty.png);
}
[class*="colorSelector"]  span {
	position: absolute;
	width: 30px;
	height: 30px;
	background: url(../images/select.png) center;
}

