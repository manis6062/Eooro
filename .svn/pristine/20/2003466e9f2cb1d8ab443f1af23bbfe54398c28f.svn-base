.bestof.sidebar-resposive .browse-category a { border-bottom: 1px solid #444444; border-right: 1px solid #4E4E4E; box-shadow: -1px 0 0 #3B3B3B inset; margin: 0; padding: 10px 15px 10px 10px;}

