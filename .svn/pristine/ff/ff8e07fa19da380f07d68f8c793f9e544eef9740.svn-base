	background-color:#ddd;
	color:#111;
    opacity:0.8;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
}


/**
* Header
*/

.navbar .nav > li > a, .search-advanced .title {
    color: #<?=SCHEME_COLORNAVBARLINK?>;
}

